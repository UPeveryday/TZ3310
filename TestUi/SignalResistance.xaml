<UserControl x:Class="TestUi.SignalResistance"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestUi"
             xmlns:testui="clr-namespace:TestUi"
             mc:Ignorable="d" 
            >
    <Grid Background="#333337">
        <Grid.ColumnDefinitions >
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <local:DashBoard x:Name="Current" Margin="0 0 10 0" MaxValue="20" FiristColor="#15e53e" SecondColor="#2575fc"  ThirdColor="#6a11cb" IsResistance="False"></local:DashBoard>
        <local:DashBoard x:Name="Resistance" Grid.Column="1" MaxValue="1000"  FiristColor="Yellow" SecondColor="#72ca12"  ThirdColor="#15e53e"  IsResistance="True"></local:DashBoard>
    </Grid>
</UserControl>
